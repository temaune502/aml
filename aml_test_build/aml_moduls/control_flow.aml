import_py {
    console
}
func classify(x){
    if (x >= 100) {
        return "large"
    }
    else {
        return "small"
    }
}
func sum_until(n){
    var s = 0
    var i = 0
    while (i < n) {
        i += 1
        if (i % 2 == 1) {
            continue
        }
        s += i
        if (s > 1000) {
            break
        }
    }
    return s
}
func for_print(xs){
    for item in xs {
        console.print_line(item)
    }
}