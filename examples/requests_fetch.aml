import_py { requests, pathlib }

meta { entry: "app.main" }

namespace app {
  func main() {
    var p = pathlib.Path("./tmp_aml")
    p.mkdir(exist_ok=True)

    var url = "https://httpbin.org/json"
    var r = requests.get(url, timeout=5)
    p.joinpath("http.json").write_text(r.text, encoding="utf-8")
  }
}