print("--- Dynamic Access Test ---")

var obj = {
    "prop1": "value1",
    "prop2": "value2"
}

var p = "prop1"
print("Access by variable p=" + p + ":", obj[p])

func get_prop(o, name) {
    return o[name]
}

print("Access via function:", get_prop(obj, "prop2"))

