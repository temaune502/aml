print("--- Built-ins and Type Conversion Test ---")

var s = "123"
var i = int(s)
var f = float(s) + 0.5

print("String:", s, "Type:", type(s))
print("Int:", i, "Type:", type(i))
print("Float:", f, "Type:", type(f))

var my_list = [1, 2, 3]
print("List length:", len(my_list))
print("String representation:", str(my_list))

// Test boolean conversions
print("Is truthy (1):", bool(1))
print("Is truthy (0):", bool(0))
print("Is truthy ([]):", bool([]))
print("Is truthy ([1]):", bool([1]))
